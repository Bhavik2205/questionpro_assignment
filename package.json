{
  "name": "questionpro",
  "version": "1.0.0",
  "description": "assignment ",
  "main": "server.js",
  "scripts": {
    "tsc": "tsc",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings=0",
    "lint-auto-fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "start": "tsc && tsc-alias && node dist",
    "build": "tsc && tsc-alias",
    "env": "sync-dotenv",
    "liquibase": "cd liquibase && liquibase update",
    "liquibase-prod": "cd liquibase && liquibase update --defaultsFile=./liquibase-prod.properties",
    "liquibase-dev": "cd liquibase && liquibase update --defaultsFile=./liquibase-dev.properties",
    "pretty:check": "prettier --check \"./**/*.{js,jsx,ts,tsx,json,md}\"",
    "pretty:write": "prettier --write \"./**/*.{js,jsx,ts,tsx,json,md}\""
  },
  "engines": {
    "npm": "`npm` is not support, Use `yarn`",
    "yarn": ">= 1.22.19",
    "node": ">=18.16.0 <=18"
  },
  "private": true,
  "author": "Bhavik Patel",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "esbuild": "^0.20.1",
    "eslint": "^8.57.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "sync-dotenv": "^2.7.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tslint": "^6.1.3"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jm-ez-l10n": "^1.0.0",
    "jsonwebtoken": "^9.0.2",
    "method-override": "^3.0.0",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "querystring": "^0.2.1",
    "reflect-metadata": "^0.2.1",
    "typeorm": "^0.3.20",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  }
}
